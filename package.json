    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^2.4.5",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "d3": "^3.5.16",
    "lodash": "^4.8.2",
    "require": "^2.4.20",
    "uuid": "^2.0.1"
