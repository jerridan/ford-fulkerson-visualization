  "main": "dist/vertex.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel src/index.js -o dist/index.js",
    "dev-build": "browserify src/index.js -o dist/index.js",
    "dev-watch": "watchify src/index.js -o dist/index.js",
